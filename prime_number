import java.util.Scanner;
public class main{
    public static void IsPrime(int n){
        boolean prime=true;
        for(int i=2;i<n-1;i++){
            if(n%i==0){
                prime=false;
                break;
            }
        }
        system.out.println(prime);
        
        
